class Solution {
public:
    vector<int> maxKDistinct(vector<int>& nums, int k) {
        int n = nums.size();
        unordered_set<int> s; // convert into set 
        for ( int i = 0; i < n; i++) {
            s.insert(nums[i]);
        }
        // set into vector
        vector<int>v(s.begin() , s.end());
// deachending order me sort
        sort(v.begin(), v.end() , greater<int>());
        
        if(v.size() > k){
            v.resize(k);
        }
        return v;
    }
};
