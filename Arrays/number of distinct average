class Solution {
public:
    int distinctAverages(vector<int>& nums) {
        sort(nums.begin() , nums.end());
        int left = 0;
        int right = nums.size()-1;
     set<double> s; // distinct averages store karne ke liye

        while(left < right){
             double avrage  = (nums[left] + nums[right]) / 2.0;
            s.insert(avrage);
            left++;
            right--;
        }
       return s.size();
    }
};
