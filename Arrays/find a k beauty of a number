class Solution {
public:
    int divisorSubstrings(int num, int k) {
       string ch = to_string(num) ;
       int n = ch.size();
       int Kbeauty = 0;

       for(int i = 0 ; i <= n-k ;i++){
string sub = ch.substr(i, k);  // length k ka substring
            int val = stoi(sub);    
            if(val != 0 && num % val == 0){
                Kbeauty++;
            }
       }
       return Kbeauty;
    }
};
