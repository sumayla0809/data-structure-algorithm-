#include <iostream>
using namespace std;
void swapaltranative(int arr[], int n)
{
    for (int i = 0; i < n; i = i + 2)
    {
        if (i + 1 < n)
        {
            swap(arr[i], arr[i + 1]);
        }
    }
}
int main()
{
    int arr[] = {1, 2, 3, 4, 5};
    int n = 5;
    swapaltranative(arr, n);
    cout << " swap altranative is" << endl;
    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }
    return 0;
}
