class Solution {
public:
    int smallestIndex(vector<int>& nums) {
        for(int i = 0 ; i < nums.size();i++){
            int x = nums[i];
        int sum = 0;
    
            while( x > 0 ){
             sum += x % 10 ;
              x = x / 10;
            }
             if(i == sum ){
                return i;
             }
        }
        return -1;
    }
};
