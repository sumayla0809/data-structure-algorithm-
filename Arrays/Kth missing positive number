class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        
        int current = 1; 
        int count = 0;
       int i = 0;
        while(true){
            if(i < arr.size() && arr[i] == current)
                i++;
            
            else{
                count++;
                if(count == k) return current;
            }
            current++;
        }


    }
    
};
