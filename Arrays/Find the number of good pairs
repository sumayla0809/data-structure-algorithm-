class Solution {
public:
    int numberOfPairs(vector<int>& nums1, vector<int>& nums2, int k) {
        int count = 0;
        for (int i = 0; i < nums1.size(); i++) {
            for (int j = 0; j < nums2.size(); j++) {
                int num1 = nums1[i];
                int num2 = nums2[j] * k;
                if (num1 % num2 == 0) {
                    count++;
                }
            }
        }
        return count;
    }
};
