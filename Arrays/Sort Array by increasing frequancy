class Solution {
public:
    vector<int> frequencySort(vector<int>& nums) {
        int n = nums.size();
        unordered_map<int , int >freq;
        for(int i = 0 ; i < n ; i ++){
freq[nums[i]]++;
        }
        int mini = INT_MAX;
        vector<int>ans;
     sort(nums.begin() , nums.end() , [&](int a , int b ){

     if(freq[a] == freq[b]){
return a > b;
     }
     else{
        return freq[a]< freq[b];
     }
     });
     return nums;
    }
};
