class Solution {
public:
    string findValidPair(string s) {
        unordered_map<int , int > freq;
        string ans =  "";
        for(int i = 0 ; i < s.length();i++){
 char ch = s[i];
freq[ch]++;
        }
        for(int i = 0 ; i <s.length();i++ ){
            if(s[i] != s[i+1]  && 
freq[s[i]] == (s[i]-'0')  && 
freq[s[i+1]] == (s[i+1]-'0')){
ans.push_back(s[i]);
ans.push_back(s[i+1]);

        return ans;

            }
        }
        return "";
    }
};
