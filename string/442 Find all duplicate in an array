class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        int n = nums.size();
        vector<int>visited(n+1 , 0);
        vector<int>ans;
        for(int i = 0 ; i < n;i++){
            if(visited[nums[i]]  == 1)
                ans.push_back(nums[i]);
            
            visited[nums[i]]  = 1;
        }
        return ans;
    }
};
