class Solution {
public:
    bool isBalanced(string num) {
        int evensum = 0;
        int oddsum = 0;
  for(int i = 0 ; i < num.size();i++){
    int digit = num[i] - '0';
    if(i % 2 == 0){
evensum += digit;
    }
    else{
        oddsum += digit;

    }
  }
return evensum == oddsum;

  
    }
};
