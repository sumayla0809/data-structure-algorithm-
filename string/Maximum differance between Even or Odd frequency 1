class Solution {
public:
int maxDifference(string s) {
    unordered_map<char, int> freq;
    for (char ch : s) {
        freq[ch]++;
    }

    int maxOdd = INT_MIN;
    int minEven = INT_MAX;

    for (auto it : freq) {
        if (it.second % 2 == 1)
            maxOdd = max(maxOdd, it.second);
        else
            minEven = min(minEven, it.second);
    }

    return (maxOdd != INT_MIN && minEven != INT_MAX) ? maxOdd - minEven : -1;
}

    
};
