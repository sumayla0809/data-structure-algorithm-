class Solution {
public:
    vector<int> findDisappearedNumbers(vector<int>& nums) {
        int n = nums.size();
        vector<bool>visited(n+1 , false);
        vector<int>ans;
        for(int i = 0 ; i < n ; i++){
           visited[nums[i]] = true;
        }
        for(int i = 1 ; i <= n ; i++){
            if(!visited[i]){
                ans.push_back(i);
            }
        }
        return ans;
    }
};
