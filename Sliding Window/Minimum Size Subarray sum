class Solution {
public:
    int minSubArrayLen(int target, vector<int>& nums) {
        int  start = 0 ,sum = 0;
        int ans =  nums.size()+1;
        for(int end = 0; end < nums.size() ; end++){
            sum += nums[end];
while(sum>= target){
ans = min(ans , end - start +1);
sum -= nums[start];
start++;
}
        }
                return (ans == nums.size() + 1) ? 0 : ans;

    }
};
